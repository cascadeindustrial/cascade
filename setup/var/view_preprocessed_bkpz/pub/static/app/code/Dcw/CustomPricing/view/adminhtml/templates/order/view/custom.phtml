<!-- <h1>Hi, I am here!</h1> --><?php $orderId = $this->getRequest()->getParam('order_id'); $objectManager = \Magento\Framework\App\ObjectManager::getInstance(); $order = $objectManager->create('Magento\Sales\Model\Order')->load($orderId); $orderItems = $order->getAllItems(); $shippingOption = 0; foreach($orderItems as $orderItem) { if($orderItem->getShippingOption()) { $shippingOption = $orderItem->getShippingOption(); $productId = $orderItem->getProductId(); $product = $objectManager->get('Magento\Catalog\Model\Product')->load($productId); break; } } ?> <?php if($shippingOption) {?> <h3>Delivery Option</h3><?php if($shippingOption==1){ echo "Standard Delivery"."<br/>"; echo $product->getAttributeText('standard_delivery_time'); } else if($shippingOption==2) { echo "Expedited Delivery"."<br/>"; echo $product->getAttributeText('expedited_delivery_time'); } } ?>