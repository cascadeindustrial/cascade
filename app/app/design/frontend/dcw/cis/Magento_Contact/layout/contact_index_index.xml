<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Contact\Block\ContactForm" name="contactFormBanner" template="Magento_Contact::formbanner.phtml">
            </block>

        </referenceContainer>
        <move element="contactFormBanner" destination="page.wrapper" before="-" />
