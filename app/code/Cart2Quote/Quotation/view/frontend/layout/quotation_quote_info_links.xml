<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Html\Links" as="links" name="quotation.quote.info.links" before="-">
                <arguments>
                    <argument name="css_class" xsi:type="string">items order-links</argument>
                </arguments>
                <block class="Cart2Quote\Quotation\Block\Quote\Link" name="quotation.quote.info.links.information">
                    <arguments>
                        <argument name="path" xsi:type="string">quotation/quote/view</argument>
                        <argument name="label" xsi:type="string">Items Requested</argument>
                    </arguments>
                </block>
                <block class="Cart2Quote\Quotation\Block\Quote\Link" name="quotation.quote.info.links.files" cacheable="false">
                    <arguments>
                        <argument name="path" xsi:type="string">quotation/quote/files</argument>
                        <argument name="label" xsi:type="string">Files Uploaded</argument>
                    </arguments>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
